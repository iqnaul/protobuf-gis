{
  "name": "protobuf-gis",
  "version": "1.0.0",
  "description": "GIS ProtoBuf module",
  "main": "index.js",
  "dependencies": {
    "google-protobuf": "^3.3.0"
  },
  "devDependencies": {},
  "scripts": {
    "build": "protoc --js_out=import_style=commonjs,binary:. -I/usr/include /usr/include/gis/protobuf/*.proto",
    "patch-coordinate": "cat patch/coordinate.js >> gis/protobuf/coordinate_pb.js",
    "patch-point": "cat patch/point.js >> gis/protobuf/point_pb.js",
    "patch-linestring": "cat patch/linestring.js >> gis/protobuf/linestring_pb.js",
    "patch-polygon": "cat patch/polygon.js >> gis/protobuf/polygon_pb.js",
    "postinstall": "npm run build; npm run patch-coordinate; npm run patch-point; npm run patch-linestring; npm run patch-polygon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomi77/protobuf-gis.git"
  },
  "keywords": [
    "gis",
    "google-protocol-buffers",
    "protobuf"
  ],
  "author": "Tomasz Jakub Rup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomi77/protobuf-gis/issues"
  },
  "homepage": "https://github.com/tomi77/protobuf-gis/js/"
}
